import java.util.*;

class Sum{

	public static void main(String[] args) {		Scanner obj = new Scanner(System.in);      

		System.out.println("Enter the first element");	

		int a = obj.nextInt();

		System.out.println("Enter the Second element");

		int b = obj.nextInt();

		int sum = a+b;

		System.out.println("Sum is: "+sum);

	}

}
